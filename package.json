{
  "name": "webpack2-react",
  "version": "1.0.0",
  "description": "react antd webpack2",
  "author": "",
  "keywords": [
    "ant",
    "react",
    "admin",
    "frontend"
  ],
  "scripts": {
    "_lint": "eslint src",
    "start": "better-npm-run start",
    "clean": "better-npm-run remove",
    "open:dist": "lite-server --config \"./dev-config/lite-server-config.js\" ",
    "build": "npm run build:dev",
    "build:dev": "better-npm-run build:dev",
    "build:dev:watch": "better-npm-run build:dev:watch",
    "build:prod": "better-npm-run build:prod",
    "tree:dir": "treee -l 2 --ignore node_modules/,.git/"
  },
  "betterScripts": {
    "start": {
      "command": "better-npm-run remove && better-npm-run dev"
    },
    "remove": {
      "command": "rimraf dist"
    },
    "dev": {
      "command": "webpack-dev-server --config \"./dev-config/webpack.config.js\" --progress --colors --hot ",
      "env": {
        "NODE_ENV": "development",
        "IS_PROXY": true
      }
    },
    "build:dev": {
      "command": "better-npm-run remove && webpack --config \"./dev-config/webpack.config.js\" --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:dev:watch": {
      "command": "better-npm-run remove && webpack --config \"./dev-config/webpack.config.js\" --progress --colors --display-error-details --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "better-npm-run remove && webpack --config \"./dev-config/webpack.config.js\" --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "classnames": "^2.2.5",
    "events": "^2.0.0",
    "fastclick": "^1.0.6",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "promise-polyfill": "7.1.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-lazyload": "^2.3.0",
    "react-mobile-datepicker": "^3.0.8",
    "react-router": "^3.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "better-npm-run": "0.0.14",
    "bitbar-webpack-progress-plugin": "^0.1.3",
    "cli-color": "^1.2.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.23.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "globby": "^6.1.0",
    "html-webpack-plugin": "^2.30.1",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "lite-server": "^2.3.0",
    "postcss-loader": "^1.3.3",
    "postcss-px2rem": "^0.3.0",
    "progress-webpack-plugin": "^0.0.12",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "qrcode": "^1.2.0",
    "react-hot-loader": "^1.3.1",
    "react-transition-group": "^2.5.0",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.1",
    "strip-loader": "^0.1.2",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tree-cli": "^0.4.12",
    "url-loader": "0.5.7",
    "webpack": "^3.6.0",
    "webpack-dev-server": "~2.7"
  },
  "repository": "https://github.com/githbq/hbq-simple-webpack2-react.git",
  "license": "MIT"
}
